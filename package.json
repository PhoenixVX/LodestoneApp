{
  "name": "lodestone-app",
  "version": "0.0.2",
  "description": "Project Lodestone App | Edit, Convert and Transform Minecraft Worlds.",
  "author": "Team Lodestone",
  "homepage": "https://team-lodestone.github.io",
  "repository": "https://github.com/Team-Lodestone/App",
  "license": "GPL-3.0",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "dev": "vite",
    "build": "npm run liblce:compile && vite build",
    "preview": "vite preview",
    "liblce:compile": "run-script-os",
    "liblce:compile:win32": ".\\deps\\scripts\\liblce-compile.bat",
    "liblce:compile:darwin:linux": "./deps/scripts/liblce-compile.sh"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@mateothegreat/svelte5-router": "^2.14.0",
    "@team-lodestone/lodestone-ui": "^1.3.9",
    "core-js": "^3.41.0",
    "electron-updater": "^6.3.9",
    "marked": "^15.0.7",
    "run-script-os": "^1.1.6",
    "svelte-i18n": "^4.0.1",
    "svelte5-router": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-optional-chaining": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/node": "^22.13.4",
    "@vitejs/plugin-legacy": "^6.0.2",
    "eslint": "^9.20.1",
    "eslint-plugin-svelte": "^2.46.1",
    "prettier": "^3.5.1",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.20.1",
    "svelte-check": "^4.1.4",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-babel": "^1.3.0",
    "vite-plugin-pwa": "^0.21.1"
  }
}
